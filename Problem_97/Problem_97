#include <iostream>
#include <cstdint>

int main()
{
    uint64_t lastTen = 1;
    for (uint64_t i = 1; i <= 7830457; i++) {
        lastTen *= 2;
        if (lastTen > 10000000000)
        {
            lastTen -= 10000000000;
        }
    }
    lastTen = (lastTen * 28433 + 1) % 10000000000;
    std::cout << lastTen << "\n";
    return 0;
}
